<%= form_for([@workout, @workout.exercises.build]) do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :sets %><br>
    <%= f.text_field :sets %>
  </div>
  <div class="field">
    <%= f.label :reps %><br>
    <%= f.text_field :reps %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>